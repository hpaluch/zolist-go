runtime: go
api_version: go1

handlers:
- url: /static
  static_dir: static
- url: /.*
  script: _go_app

skip_files:
- ^(.*/)?#.*#$
- ^(.*/)?.*~$
- .*\.(md|sh)$
- ^\.gitignore$
- ^\.git/.*
- ^_scripts/.*

# these files are imported as src/github.com/hpaluch/...
# and may not be part of app
nobuild_files:
- ^zolist/.*\.go$

